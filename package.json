{
  "private": true,
  "name": "janus-trainer",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "backend": "yarn workspace janus-trainer-backend",
    "clean": "yarn dto clean && yarn backend clean && yarn frontend clean",
    "frontend": "yarn workspace janus-trainer-frontend",
    "dto": "yarn workspace janus-trainer-dto",
    "build": "yarn build:dto && yarn build:backend && yarn build:backend",
    "build:backend": "yarn backend build",
    "build:dto": "yarn dto build",
    "start:backend": "yarn workspace janus-trainer-backend start:dev",
    "start:frontend": "yarn workspace janus-trainer-frontend start:dev",
    "start": "concurrently --kill-others-on-fail \"yarn start:backend\" \"yarn start:frontend\"",
    "format": "eslint .",
    "format:fix": "eslint . --fix",
    "lint": "yarn workspace janus-trainer-frontend run lint",
    "prepare": "husky",
    "test:e2e": "yarn workspace janus-trainer-e2e test"
  },
  "packageManager": "yarn@4.1.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.7",
    "prettier": "^3.2.4"
  }
}
