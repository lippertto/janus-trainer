{
  "name": "janus-trainer-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build --webpack",
    "start:dev": "yarn run build && serverless offline",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --projects ./test",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate:test": "dotenv -e .env.development -- typeorm-ts-node-commonjs migration:generate --dataSource datasource.ts",
    "migration:run:test": "dotenv -e .env.development -- typeorm-ts-node-commonjs migration:run --dataSource datasource.ts",
    "migration:run:prod": "dotenv -e .env.production -- typeorm-ts-node-commonjs migration:run --dataSource datasource.ts",
    "deploy": "yarn run build && serverless deploy --stage production"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.485.0",
    "@codegenie/serverless-express": "^4.13.0",
    "@nestjs/common": "^10",
    "@nestjs/core": "^10",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10",
    "@nestjs/platform-express": "^10",
    "@nestjs/typeorm": "^10",
    "@testcontainers/postgresql": "^10.4.0",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "janus-trainer-dto": "*",
    "jwks-rsa": "^3.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10",
    "@nestjs/schematics": "^10",
    "@nestjs/testing": "^10",
    "@types/aws-lambda": "^8.10.131",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "serverless-offline": "^13.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src",
      "test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
